0.0.0.0:2015
on startup daphne sample:application &

header / {
    Referrer-Policy "same-origin"
    X-XSS-Protection "1; mode=block"
    X-Content-Type-Options "nosniff"
    X-Frame-Options "DENY"
    -Server
}

proxy / localhost:8000 {
    transparent
    websocket
    except /static
}

limits 750000000
log / stdout "{combined}"
errors stdout
